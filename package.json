{
  "name": "mxp",
  "version": "0.5.2",
  "description": "Method: Expedition - Knowledge Engine for Visionary Projects",
  "main": "./dist/mxp.js",
  "type": "commonjs",
  "bin": {
    "mxp": "./mxp"
  },
  "files": [
    "dist",
    "src",
    "mxp",
    "openapi.json"
  ],
  "scripts": {
    "start": "node ./mxp.js",
    "dev:api": "tsx watch server.ts",
    "dev:vite": "vite",
