    "@mui/material": "^5.15.12",
    "@types/js-yaml": "^4.0.9",
    "@types/open": "^6.1.0",
    "art-standard-lib": "^1.74.3",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "gray-matter": "^4.0.3",
    "js-yaml": "^4.1.0",
    "open": "^10.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^9.0.1",
    "react-syntax-highlighter": "^15.5.0",
