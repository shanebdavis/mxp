    "@types/uuid": "^9.0.8",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "@vitest/coverage-v8": "^3.0.6",
    "@vitest/ui": "^3.0.5",
    "concurrently": "^8.2.2",
    "jsdom": "^26.0.0",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3",
